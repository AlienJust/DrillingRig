				<TextBlock
					Grid.Column="0"
					Grid.Row="9"
					Text="{Binding McwReceived8, Converter={StaticResource NbConverter}, Mode=OneWay}"
					FontSize="14"
					VerticalAlignment="Center" />

				<CheckBox
					Grid.Column="1"
					Grid.Row="10"
					Content="INCHING2"
					IsChecked="{Binding Inching2, Mode=OneWayToSource}"
					VerticalAlignment="Center" />
				<TextBlock
					Grid.Column="0"
					Grid.Row="10"
					Text="{Binding McwReceived9, Converter={StaticResource NbConverter}, Mode=OneWay}"
					FontSize="14"
					VerticalAlignment="Center" />

				<CheckBox
					Grid.Column="1"
					Grid.Row="11"
					Content="REMOTE"
					IsChecked="{Binding Remote, Mode=OneWayToSource}"
					VerticalAlignment="Center" />
				<TextBlock
					Grid.Column="0"
					Grid.Row="11"
					Text="{Binding McwReceived10, Converter={StaticResource NbConverter}, Mode=OneWay}"
					FontSize="14"
					VerticalAlignment="Center" />



				<TextBlock
					Grid.Row="0"
					Grid.Column="2"
					Text="MSW:"
					FontWeight="Bold"
					Grid.ColumnSpan="2" />
				<TextBlock
					Grid.Column="3"
					Grid.Row="1"
					Text="RDY_ON"
					VerticalAlignment="Center" />
				<TextBlock
					Grid.Column="2"
					Grid.Row="1"
					Text="{Binding MswReceived0, Converter={StaticResource NbConverter}, Mode=OneWay}"
					FontSize="14"
					VerticalAlignment="Center" />

				<TextBlock
					Grid.Column="3"
					Grid.Row="2"
					Text="RDY_RUN"
					VerticalAlignment="Center" />
				<TextBlock
					Grid.Column="2"
					Grid.Row="2"
					Text="{Binding MswReceived1, Converter={StaticResource NbConverter}, Mode=OneWay}"
					FontSize="14"
					VerticalAlignment="Center" />

				<TextBlock
					Grid.Column="3"
					Grid.Row="3"
					Text="RDY_REF"
					VerticalAlignment="Center" />
				<TextBlock
					Grid.Column="2"
					Grid.Row="3"
					Text="{Binding MswReceived2, Converter={StaticResource NbConverter}, Mode=OneWay}"
					FontSize="14"
					VerticalAlignment="Center" />

				<TextBlock
					Grid.Column="3"
					Grid.Row="4"
					Text="TRIPPED"
					VerticalAlignment="Center" />
				<TextBlock
					Grid.Column="2"
					Grid.Row="4"
					Text="{Binding MswReceived3, Converter={StaticResource NbConverter}, Mode=OneWay}"
					FontSize="14"
					VerticalAlignment="Center" />

				<TextBlock
					Grid.Column="3"
					Grid.Row="5"
					Text="OFF_2STA"
					VerticalAlignment="Center" />
				<TextBlock
					Grid.Column="2"
					Grid.Row="5"
					Text="{Binding MswReceived4, Converter={StaticResource NbConverter}, Mode=OneWay}"
					FontSize="14"
					VerticalAlignment="Center" />

				<TextBlock
					Grid.Column="3"
					Grid.Row="6"
					Text="OFF_3STA"
					VerticalAlignment="Center" />
				<TextBlock
					Grid.Column="2"
					Grid.Row="6"
					Text="{Binding MswReceived5, Converter={StaticResource NbConverter}, Mode=OneWay}"
					FontSize="14"
					VerticalAlignment="Center" />

				<TextBlock
					Grid.Column="3"
					Grid.Row="7"
					Text="ON_INHIBITED"
					VerticalAlignment="Center" />
				<TextBlock
					Grid.Column="2"
					Grid.Row="7"
					Text="{Binding MswReceived6, Converter={StaticResource NbConverter}, Mode=OneWay}"
					FontSize="14"
					VerticalAlignment="Center" />

				<TextBlock
					Grid.Column="3"
					Grid.Row="8"
					Text="ALARM"
					VerticalAlignment="Center" />
				<TextBlock
					Grid.Column="2"
					Grid.Row="8"
					Text="{Binding MswReceived7, Converter={StaticResource NbConverter}, Mode=OneWay}"
					FontSize="14"
					VerticalAlignment="Center" />

				<TextBlock
					Grid.Column="3"
					Grid.Row="9"
					Text="AT_SETPOINT"
					VerticalAlignment="Center" />
				<TextBlock
					Grid.Column="2"
					Grid.Row="9"
					Text="{Binding MswReceived8, Converter={StaticResource NbConverter}, Mode=OneWay}"
					FontSize="14"
					VerticalAlignment="Center" />

				<TextBlock
					Grid.Column="3"
					Grid.Row="10"
					Text="REMOTE"
					VerticalAlignment="Center" />
				<TextBlock
