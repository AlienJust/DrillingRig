			</Grid>
			<Grid
				Grid.Column="1">
				<Grid.RowDefinitions>
					<RowDefinition />
					<RowDefinition />
					<RowDefinition />
					<RowDefinition />
					<RowDefinition />
					<RowDefinition />
					<RowDefinition />
				</Grid.RowDefinitions>

				<Button
					Grid.Row="0"
					Grid.Column="0"
					Command="{Binding SendAinCommandOff1, Mode=OneTime}"
					HorizontalContentAlignment="Right">
					<TextBlock
						Text="Штатный останов (OFF 1)" />
				</Button>
				<Button
					Grid.Row="1"
					Grid.Column="0"
					Command="{Binding SendAinCommandOff2, Mode=OneTime}"
					HorizontalContentAlignment="Right">
					<TextBlock
						Text="Выбег (OFF 2)" />
				</Button>
				<Button
					Grid.Row="2"
					Grid.Column="0"
					Command="{Binding SendAinCommandOff3, Mode=OneTime}"
