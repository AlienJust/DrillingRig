				<xctk:IntegerUpDown Grid.Column="1" Value="{Binding Path=DecDfDt, Mode=TwoWay}"/>
			</Grid>
			<Grid>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="3*"/>
					<ColumnDefinition Width="1*"/>
