				<xctk:IntegerUpDown Grid.Column="1" Value="{Binding Path=Empty53, Mode=TwoWay}"/>
			</Grid>
			<Grid>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="3*"/>
					<ColumnDefinition Width="1*"/>
				</Grid.ColumnDefinitions>
				<TextBlock Grid.Column="0" VerticalAlignment="Center" Text="Темп спада частоты при аварийном останове привода"/>
