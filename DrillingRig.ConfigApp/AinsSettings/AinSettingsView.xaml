				<xctk:IntegerUpDown Grid.Column="1" Value="{Binding Path=IdSetMax , Mode=TwoWay}"/>
			</Grid>
			<Grid>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="3*"/>
					<ColumnDefinition Width="1*"/>
				</Grid.ColumnDefinitions>
				<TextBlock Grid.Column="0" VerticalAlignment="Center" Text="Пропорциональный коэф. регулятора компенсатора"/>
				<xctk:IntegerUpDown Grid.Column="1" Value="{Binding Path=KpFe , Mode=TwoWay}"/>
			</Grid>
			<Grid>
				<Grid.ColumnDefinitions>
