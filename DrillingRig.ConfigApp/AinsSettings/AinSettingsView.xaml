						<ColumnDefinition
							Width="1*" />
					</Grid.ColumnDefinitions>
					<TextBlock
						Grid.Column="0"
						VerticalAlignment="Center"
						Text="51. rsvd" />
					<xctk:IntegerUpDown
						Grid.Column="1"
						Value="{Binding Path=Reserved51, Mode=TwoWay}"
						FormatString="X4"
						Minimum="0"
						Maximum="65535" />
				</Grid>

				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition
							Width="3*" />
						<ColumnDefinition
							Width="1*" />
					</Grid.ColumnDefinitions>
					<TextBlock
						Grid.Column="0"
						VerticalAlignment="Center"
						Text="52.0-4. Np - Число пар полюсов (не путать с числом полюсов) АД" />
					<xctk:ShortUpDown
						Grid.Column="1"
						Value="{Binding Path=Np, Mode=TwoWay}" />
				</Grid>

				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition
							Width="3*" />
						<ColumnDefinition
							Width="1*" />
					</Grid.ColumnDefinitions>
					<TextBlock
						Grid.Column="0"
						VerticalAlignment="Center"
						Text="52.5-7. NimpFloorCode - Код числа импульсов ДЧВ" />
					<xctk:IntegerUpDown
						Grid.Column="1"
						Value="{Binding Path=NimpFloorCode, Mode=TwoWay}" />
				</Grid>

				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition
							Width="3*" />
						<ColumnDefinition
							Width="1*" />
					</Grid.ColumnDefinitions>
					<TextBlock
						Grid.Column="0"
						VerticalAlignment="Center"
						Text="52.8-9. FanMode - Режим работы вентилятора" />
					<xctk:IntegerUpDown
						Grid.Column="1"
						Value="{Binding Path=FanMode, Mode=TwoWay}" Minimum="0" Maximum="3"/>
				</Grid>


				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition
							Width="3*" />
						<ColumnDefinition
							Width="1*" />
					</Grid.ColumnDefinitions>
					<TextBlock
						Grid.Column="0"
						VerticalAlignment="Center"
						Text="53. u_mod_thr - Порог компенсации напряжения DC за счет потока" />
					<xctk:DoubleUpDown
						Grid.Column="1"
						Value="{Binding Path=UmodThr, Mode=TwoWay}" />
				</Grid>

				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition
							Width="3*" />
						<ColumnDefinition
							Width="1*" />
					</Grid.ColumnDefinitions>
					<TextBlock
						Grid.Column="0"
						VerticalAlignment="Center"
						Text="54. EMDEC_Dfdt - Темп спада частоты при аварийном останове привода" />
					<xctk:ShortUpDown
						Grid.Column="1"
						Value="{Binding Path=EmdecDfdt , Mode=TwoWay}" />
				</Grid>

				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition
							Width="3*" />
						<ColumnDefinition
							Width="1*" />
					</Grid.ColumnDefinitions>
					<TextBlock
						Grid.Column="0"
						VerticalAlignment="Center"
						Text="55. Text_max - Порог защиты по внешней температуре" />
					<xctk:ShortUpDown
						Grid.Column="1"
						Value="{Binding Path=TextMax , Mode=TwoWay}" />
				</Grid>

				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition
							Width="3*" />
						<ColumnDefinition
							Width="1*" />
					</Grid.ColumnDefinitions>
					<TextBlock
						Grid.Column="0"
						VerticalAlignment="Center"
						Text="56. TO_HL - Таймаут по системной линии связи" />
					<xctk:ShortUpDown
						Grid.Column="1"
						Value="{Binding Path=ToHl , Mode=TwoWay}" />
				</Grid>
			</StackPanel>
		</ScrollViewer>
		<Grid
			Grid.Row="1">
			<Grid.ColumnDefinitions>
				<ColumnDefinition />
				<ColumnDefinition />
				<ColumnDefinition />
				<ColumnDefinition />
			</Grid.ColumnDefinitions>
			<Button
				Grid.Column="0"
				Content="Импорт..."
				Command="{Binding Path=ImportSettingsCommand, Mode=OneTime}" />
			<Button
				Grid.Column="1"
				Content="Экспорт..."
				Command="{Binding Path=ExportSettingsCommand, Mode=OneTime}" />
			<Button
				Grid.Column="2"
				Content="Прочитать"
				Command="{Binding Path=ReadSettingsCommand, Mode=OneTime}" />
			<Button
				Grid.Column="3"
				Content="Записать"
				Command="{Binding Path=WriteSettingsCommand, Mode=OneTime}" />
		</Grid>
	</Grid>
</UserControl>
