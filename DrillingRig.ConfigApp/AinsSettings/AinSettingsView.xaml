				<xctk:IntegerUpDown Grid.Column="1" Value="{Binding Path=Unom, Mode=TwoWay}"/>
			</Grid>
			<Grid>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="3*"/>
					<ColumnDefinition Width="1*"/>
				</Grid.ColumnDefinitions>
				<TextBlock Grid.Column="0" VerticalAlignment="Center" Text="Пустой параметр №39"/>
				<xctk:IntegerUpDown Grid.Column="1" Value="{Binding Path=Empty39, Mode=TwoWay}"/>
			</Grid>
			<Grid>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="3*"/>
					<ColumnDefinition Width="1*"/>
				</Grid.ColumnDefinitions>
				<TextBlock Grid.Column="0" VerticalAlignment="Center" Text="Активное сопротивление статора"/>
				<xctk:IntegerUpDown Grid.Column="1" Value="{Binding Path=Rs, Mode=TwoWay}"/>
			</Grid>
			<Grid>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="3*"/>
					<ColumnDefinition Width="1*"/>
				</Grid.ColumnDefinitions>
				<TextBlock Grid.Column="0" VerticalAlignment="Center" Text="Минимальная частота (электрическая)"/>
				<xctk:IntegerUpDown Grid.Column="1" Value="{Binding Path=Fmin, Mode=TwoWay}"/>
			</Grid>
			<Grid>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="3*"/>
					<ColumnDefinition Width="1*"/>
				</Grid.ColumnDefinitions>
				<TextBlock Grid.Column="0" VerticalAlignment="Center" Text="Постоянная времени фильтра момента"/>
				<xctk:IntegerUpDown Grid.Column="1" Value="{Binding Path=TauM, Mode=TwoWay}"/>
			</Grid>
			<Grid>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="3*"/>
					<ColumnDefinition Width="1*"/>
				</Grid.ColumnDefinitions>
				<TextBlock Grid.Column="0" VerticalAlignment="Center" Text="Постоянная времени фильтра частоты"/>
				<xctk:IntegerUpDown Grid.Column="1" Value="{Binding Path=TauF , Mode=TwoWay}"/>
			</Grid>
			<Grid>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="3*"/>
					<ColumnDefinition Width="1*"/>
