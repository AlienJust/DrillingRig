<UserControl x:Class="DrillingRig.ConfigApp.LookedLikeAbb.Chart.ChartView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DrillingRig.ConfigApp.LookedLikeAbb.Chart"
             xmlns:views="clr-namespace:RPD.SciChartControl.Views;assembly=RPD.SciChartControl"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
	<views:ChartControl
								AnalogSeries="{Binding AnalogSeries}"
								DiscreteSeries="{Binding DiscreteSeries}"
								AnalogSeriesAdditionalData="{Binding AnalogSeriesAdditionalData}"
								DiscreteSeriesAdditionalData="{Binding DiscreteSeriesAdditionalData}"/>
</UserControl>
